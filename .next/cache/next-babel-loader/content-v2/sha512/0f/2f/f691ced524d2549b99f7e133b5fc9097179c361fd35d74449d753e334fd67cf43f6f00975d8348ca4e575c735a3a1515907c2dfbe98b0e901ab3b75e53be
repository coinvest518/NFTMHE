RE__*/_jsx(\"div\", {\n          onClick: () => {\n            setScope('Followers');\n            setExpand(false);\n          },\n          className: returnActiveTitle('Followers'),\n          children: \"Followers\"\n        }), /*#__PURE__*/_jsx(\"div\", {\n          className: returnActiveNumber('Followers'),\n          children: followersAmount\n        })]\n      }), /*#__PURE__*/_jsxs(\"div\", {\n        className: style.Section,\n        children: [/*#__PURE__*/_jsx(\"div\", {\n          onClick: () => {\n            setScope('Followed');\n            setExpand(false);\n          },\n          className: returnActiveTitle('Followed'),\n          children: \"Followed\"\n        }), /*#__PURE__*/_jsx(\"div\", {\n          className: returnActiveNumber('Followed'),\n          children: followedAmount\n        })]\n      }), /*#__PURE__*/_jsxs(\"div\", {\n        onClick: disconnect,\n        className: style.Logoff,\n        children: [/*#__PURE__*/_jsx(\"div\", {\n          className: style.SectionTitle,\n          children: \"Disconnect\"\n        }), /*#__PURE__*/_jsx(PowerOff, {\n          className: style.PowerOff\n        })]\n      }), /*#__PURE__*/_jsx(\"div\", {\n        className: style.Close,\n        onClick: () => setExpand(false),\n        children: /*#__PURE__*/_jsx(Close, {\n          onClick: () => true,\n          className: style.CloseSVG\n        })\n      })]\n    })\n  });\n};\n\nexport default FloatingMenu;","map":null,"metadata":{},"sourceType":"module"}